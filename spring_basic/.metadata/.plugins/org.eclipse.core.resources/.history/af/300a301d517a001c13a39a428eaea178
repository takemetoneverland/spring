package basic.quiz;

import org.springframework.beans.factory.annotation.Autowired;

public class Computer {
	
	@Autowired //자동 주입
	private Mouse mouse;
	@Autowired
	private Keyboard keyboard;
	@Autowired
	private Monitor monitor;

	public void computerInfo() {
		mouse.info();
		keyboard.info();
		monitor.info();
		}
	}
